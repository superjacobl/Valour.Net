{
    "metadata": [
      {
        "src": [
          {
            "src": "../Valour.Net",
            "files": [
              "**/*.csproj"
            ]
          }
        ],
        "dest": "api",
        "disableGitFeatures": false,
        "disableDefaultFilter": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "api/**.yml",
            "api/index.md"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "../docs",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "templates/darkfx"
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
  }